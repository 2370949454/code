			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="false" />
		</javaModelGenerator>

		<!-- //////////////////////////////////////////////////////////////////////////////////////////////////////////////// -->
		<!-- gem annotated mapper -->
		<!-- data -->
		<javaClientGenerator type="ANNOTATEDMAPPER"
							 targetPackage="com.lsm1998.chat.mapper"
							 targetProject="C:/Users/Administrator/IdeaProjects/code/project/microChat/src/main/java">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>

